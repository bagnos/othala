		b.setLanguages("it");
		b.setDescription("prova prova");
		c.add(b);
		
		a.setLangDescription(c);
		
		a.setIdGender(1);
		a.setPgGender(1);
		a.setIdBrand(5);
		a.setPgBrand(4);
		a.setIdType(2);
		a.setPgType(14);
		
		ArticleFullDTO d = new ArticleFullDTO();
		List<ArticleFullDTO> e = new ArrayList<ArticleFullDTO>();
		d.setPgArticle(1);
		d.setQtStock(2);
		d.setIdSize(4);
		d.setPgSize(4);
		d.setIdColor(3);
		d.setPgColor(1);
		ArticleFullDTO f = new ArticleFullDTO();
		f.setPgArticle(2);
		f.setQtStock(2);
		f.setIdSize(4);
		f.setPgSize(5);
		f.setIdColor(3);
		f.setPgColor(2);
		
		e.add(d);
		e.add(f);
		
		a.setArticles(e);
		
		List<String> g = new ArrayList<String>(); 
		g.add("prova1");
