		
		
		
		
		ProductFullDTO a = new ProductFullDTO();
		a.setPrice(new BigDecimal(100));
		a.setDiscount(10);
		a.setThumbnailsUrl("antani");
		
		DescriptionDTO b = new DescriptionDTO();
